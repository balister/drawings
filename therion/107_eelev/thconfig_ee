encoding  utf-8
source gemse_ee.th
 input thconfig_ee_symbols

layout gemse
  map-header 50 0 se  
  copy symbols
#debug on
  legend on
#  colour map-fg [scrap]
  colour map-fg [90]
 # colour map-fg [altitude]

symbol-show point station
symbol-hide group cave-centreline
symbol-show point station-name
debug station-names

code tex-map
 \cavename={1623/107 Gemseh√∂hle}
 \cavelength{3766\thinspace{}m}
 \cavedepth{287\thinspace{}m}
 
  scale 1  500
  base-scale 1 200
 
  code metapost
    map-comment "Extended Elevation"
    fonts_setup(7,10,14,20,26); 
  endcode

endlayout

select all_elev.map

export map -layout gemse -projection extended -output 107-ee.pdf
