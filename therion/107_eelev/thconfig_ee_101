encoding  utf-8
source 101_ee.th
  input thconfig_ee_symbols

layout 101
  map-header 0 -20 nw
  copy symbols

#debug on
  legend on
 # colour map-fg [altitude]
#  colour map-fg [scrap]
colour map-fg [90]
symbol-hide group cave-centreline

#debug station-names

code tex-map
 \cavename={1623/101}
 \cavelength{33\thinspace{}m (surveyed)}
  \cavedepth{12\thinspace{}m (surveyed)}
#\legendtextsize={\size[10]}
\legendtextheadersize={\size[16]}
\legendwidth=10cm
 \framethickness=0.5mm

          
  endcode
  
    code metapost
    map-comment "Extended Elevation"
  endcode


  scale 1 100

endlayout
select ent.map@101

export map -layout 101 -proj extended -output 101-ee.pdf
