encoding  utf-8
source 16-jb-01_ee.th
  input thconfig_ee_symbols

layout 16-jb-01
  map-header 100 40 ne
  copy symbols

#debug on
  legend on
 # colour map-fg [altitude]
#  colour map-fg [scrap]
colour map-fg [90]
#symbol-hide group cave-centreline

#debug station-names

code tex-map
 \cavename={1623/16-jb-01}
 \cavelength{309\thinspace{}m}
  \cavedepth{120\thinspace{}m}
#\legendtextsize={\size[10]}
\legendtextheadersize={\size[16]}
\legendwidth=10cm
 \framethickness=0.5mm

          
  endcode
  
    code metapost
    map-comment "Extended Elevation"
  endcode


  scale 1 500

endlayout
select all_elev.map@all

export map -layout 16-jb-01 -proj extended -output 16-jb-01-ee.pdf
