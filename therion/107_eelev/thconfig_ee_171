encoding  utf-8
source 171_ee.th
  input thconfig_ee_symbols

layout 171
  map-header 0 50 nw
  copy symbols

#debug on
  legend on
 # colour map-fg [altitude]
#  colour map-fg [scrap]
colour map-fg [90]
symbol-hide group cave-centreline

#debug station-names

code tex-map
 \cavename={1623/171}
 \cavelength{198\thinspace{}m}
  \cavedepth{22\thinspace{}m}
#\legendtextsize={\size[10]}
\legendtextheadersize={\size[16]}
\legendwidth=10cm
 \framethickness=0.5mm

          
  endcode
  
    code metapost
    map-comment "Extended Elevation"
  endcode


  scale 1 150

endlayout
select all_elev.map@all

export map -layout 171 -proj extended -output 171-ee.pdf
