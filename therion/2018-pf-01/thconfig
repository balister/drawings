encoding  utf-8
source 2018-pf-01.th
layout xvi-export
  scale 1 100
  grid-size 1 1 1 m
endlayout
export map -fmt xvi -layout xvi-export -o irurixo-plan.xvi
select 2018-pf-01
export map -fmt xvi -layout xvi-export -proj [elevation 270] -o irurixo-elev.xvi
export map -fmt xvi -layout xvi-export -proj [elevation ] -o irurixo-xc.xvi
export map -o 2018-pf-01.pdf \
  -layout local
 export map -o 2018-pf-01-elev.pdf -proj [elevation 270] \
  -layout local

layout local
map-comment "Surveyed by Paul Fox and Natalie Uomini"
legend on
scale 1 50
endlayout
