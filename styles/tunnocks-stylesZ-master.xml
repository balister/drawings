	<subsetattr name="gridlines100m" areacolour="none" areamaskcolour="none" labelcolour="#ff000000" strokecolour="#ff000000" uppersubset="basevisiblegrid">
		<labelfcol labelstylename="gridnum" fontname="Arial" fontstyle="PLAIN" size="40"/>
		<labelfcol labelstylename="titlename" fontname="Arial" fontstyle="BOLD" size="120"/>
		<labelfcol labelstylename="titlesubname" fontname="Arial" fontstyle="BOLD" size="60"/>
