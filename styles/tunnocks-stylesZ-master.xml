	<subsetattr name="surfaceshafts" uppersubset="basevisiblecave" areacolour="none" areamaskcolour="none">
		<linestylecol linestyle="filled" strokewidth="0.0" strokecolour="#ff000000"/>
		<linestylecol linestyle="estwall" strokewidth="3.75" strokecolour="#ff000000" spikegap="22.5" gapleng="11.25" spikeheight="0.0"/>
		<labelfcol labelstylename="entrance" labelcolour="#ff000000" fontname="Arial" fontstyle="PLAIN" size="40"/>
		<symbolaut dname="entrance" description="entrance arrow">
			<asymbol name="ent" position="endpath" scale="fixed" picscale="0.5" orientation="fixed" area-interaction="allowed-outside"/>
		</symbolaut>
	</subsetattr>
	
	<subsetattr name="gridlines100m">
		<linestylecol linestyle="wall" strokewidth="4.0" strokecolour="#ff000000"/>
		<linestylecol linestyle="detail" strokewidth="4.0" strokecolour="#ff000000"/>
		<labelfcol labelstylename="titlecreditslarge" labelcolour="none"/>
		<labelfcol labelstylename="titlestats" labelcolour="none"/>
		<labelfcol labelstylename="titlestatcols" labelcolour="none"/>
	</subsetattr>


</groupsubsetattr>

</fontcolours>

