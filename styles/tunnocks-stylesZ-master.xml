			<linestylecol linestyle="wall" strokewidth="4.0"/>
			<linestylecol linestyle="estwall" strokewidth="4.0"/>
			<linestylecol linestyle="pitchbound" strokewidth="2.5" spikegap="10.0" gapleng="0.0" spikeheight="8.0"/>
		</subsetattr>	

	<set labelcolour="#ff000000">
	
		<subsetattr name="smallscalelabels" labelcolour="#ff000000">
            <labelfcol labelstylename="titlename" fontname="Arial" fontstyle="BOLD" size="150"/>
			<labelfcol labelstylename="titlearea" fontname="Arial" fontstyle="BOLD" size="90"/>
            <labelfcol labelstylename="titlecreditslarge" fontname="Arial" fontstyle="PLAIN" size="45"/>
            <labelfcol labelstylename="titlenortharrow" fontname="Times New Roman" fontstyle="PLAIN" size="80"/>
