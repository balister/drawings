			<labelfcol labelstylename="passage" fontstyle="PLAIN" size="60"/>
			<labelfcol labelstylename="area" fontname="Arial" fontstyle="ITALIC" size="100"/>
			<labelfcol labelstylename="entrance" fontname="Arial" size="60.0"/>
			<labelfcol labelstylename="cave"/>
