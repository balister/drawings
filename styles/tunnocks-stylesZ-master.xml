	</subsetattr>

	<subsetattr name="subway" uppersubset="invisible" areacolour="#22cccccc" areamaskcolour="#22dddddd" strokecolour="#66444444">
		<linestylecol linestyle="wall"/>
		<linestylecol linestyle="estwall"/>
	</subsetattr>
	
	<subsetattr name="elev1" uppersubset="level1"/>
	<subsetattr name="elev2" uppersubset="level2"/>
	<subsetattr name="elev3" uppersubset="level3"/>
	<subsetattr name="elev4" uppersubset="level4"/>
	<subsetattr name="elev5" uppersubset="level5"/>
	<subsetattr name="elev6" uppersubset="level6"/>
