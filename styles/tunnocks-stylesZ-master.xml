		<linestylecol linestyle="detail" strokewidth="0.625" strokecolour="#ff888888"/>
	</subsetattr>

	<subsetattr name="surfaceshafts" uppersubset="basevisiblecave" areacolour="none" areamaskcolour="none">
		<linestylecol linestyle="filled" strokewidth="0.0" strokecolour="#cce50033"/>
		<linestylecol linestyle="estwall" strokewidth="3.75" strokecolour="#ffe50033" spikegap="22.5" gapleng="11.25" spikeheight="0.0"/>
		<labelfcol labelstylename="entrance" labelcolour="#ffe50033" fontname="Arial" fontstyle="BOLD" size="23"/>
		<symbolaut dname="entrance" description="entrance arrow">
			<asymbol name="ent" position="endpath" scale="fixed" picscale="0.5" orientation="fixed" area-interaction="allowed-outside"/>
		</symbolaut>
	</subsetattr>

</groupsubsetattr>

<groupsubsetattr groupsubsetname="Tunnocks only">
	<importgroupsubsetattr groupsubsetname="204areas_hierarchy"/>
    <subsetattr name="tunnocks" uppersubset="visible">
		<labelfcol labelstylename="passage" fontname="Arial" fontstyle="PLAIN"/>
		<labelfcol labelstylename="area" fontname="Arial" fontstyle="ITALIC"/>
		<labelfcol labelstylename="qm" fontname="Courier" size="200"/>
		<labelfcol labelstylename="step" fontname="Times New Roman" fontstyle="ITALIC" size="17"/>
		<labelfcol labelstylename="entrance" fontname="Arial" size="30.0"/>
		<labelfcol labelstylename="cave" fontname="Arial" fontstyle="BOLD" size="50"/>
	</subsetattr>
</groupsubsetattr>

<groupsubsetattr groupsubsetname="smallscale">	
	<importgroupsubsetattr groupsubsetname="CUCCplanes"/>
		
	<subsetattr name="basevisiblecave">
    <set multiplicity="1" scale="fixed" buttonaction="overwrite" position="endpath" orientation="random" area-interaction="allowed-outside">
		<symbolaut dname="breeze" description="breeze symbol from tunks">
            <asymbol name="breeze" position="endpath" scale="fixed" picscale="2.8" orientation="alongaxis" area-interaction="allowed-outside"/>
		</symbolaut>

		<symbolaut dname="slope" description="slope symbol">
			<asymbol name="slope" picscale="2.0"/>
		</symbolaut>
        
			<symbolaut dname="stream" description="stream symbol">
				<asymbol name="stream" position="endpath" scale="fixed" orientation="nearaxis" area-interaction="allowed-outside" picscale="4.5"/>
			</symbolaut>

        
        <symbolaut dname="popcorn" description="sweet or salty">
			<asymbol name="popcorn" picscale="4.0" orientation="fixed"/>
        </symbolaut>
        
		<symbolaut dname="stalactite" description="Stalactite">
			<asymbol name="stalactite" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim" picscale="4.5"/>
		</symbolaut>
           
		<symbolaut dname="stalagmite" description="Stalagmite">
			<asymbol name="stalagmite" position="endpath" scale="fixed" orientation="fixed" area-interaction="trim" picscale="4.5"/>
		</symbolaut>
        
    </set>    
	</subsetattr>

	

		<subsetattr name="level1" areacolour="#88444444"shadowstrokewidth="8.0" shadowstrokecolour="#ffffffff">
			<labelfcol labelstylename="qm" labelcolour="none" />
            <labelfcol labelstylename="step" fontname="Times New Roman" fontstyle="ITALIC" size="30"/>
			<labelfcol labelstylename="passage" fontstyle="PLAIN" size="60"/>
			<labelfcol labelstylename="area" fontname="Arial" fontstyle="ITALIC" size="100"/>
			<labelfcol labelstylename="entrance" fontname="Arial" size="60.0"/>
			<labelfcol labelstylename="cave"/>
			<linestylecol linestyle="centreline" strokewidth="2.0" strokecolour="#ff000000"/>
			<linestylecol linestyle="detail" strokewidth="2.5" strokecolour="#ff000000"/>
			<linestylecol linestyle="wall" strokewidth="4.0"/>
			<linestylecol linestyle="estwall" strokewidth="4.0"/>
			<linestylecol linestyle="pitchbound" strokewidth="2.5" spikegap="10.0" gapleng="0.0" spikeheight="8.0"/>
		</subsetattr>
		<subsetattr name="level2" areacolour="#88666666"shadowstrokewidth="8.0" shadowstrokecolour="#ffffffff">
            <labelfcol labelstylename="qm" labelcolour="none" />
            <labelfcol labelstylename="step" fontname="Times New Roman" fontstyle="ITALIC" size="30"/>
			<labelfcol labelstylename="passage" fontstyle="PLAIN" size="60"/>
			<labelfcol labelstylename="area" fontname="Arial" fontstyle="ITALIC" size="100"/>
			<labelfcol labelstylename="entrance" fontname="Arial" size="60.0"/>
			<labelfcol labelstylename="cave"/>
			<linestylecol linestyle="centreline" strokewidth="2.0" strokecolour="#ff000000"/>
			<linestylecol linestyle="detail" strokewidth="2.5" strokecolour="#ff000000"/>
			<linestylecol linestyle="wall" strokewidth="4.0"/>
			<linestylecol linestyle="estwall" strokewidth="4.0"/>
			<linestylecol linestyle="pitchbound" strokewidth="2.5" spikegap="10.0" gapleng="0.0" spikeheight="8.0"/>
		</subsetattr>	
		<subsetattr name="level3" areacolour="#88888888"shadowstrokewidth="8.0" shadowstrokecolour="#ffffffff">
