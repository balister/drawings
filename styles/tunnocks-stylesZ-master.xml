            <labelfcol labelstylename="step" fontname="Times New Roman" fontstyle="PLAIN" size="17"/>
            <labelfcol labelstylename="area" fontname="Arial" fontstyle="ITALIC" size="50.0"/>
			<labelfcol labelstylename="cave" fontname="Arial" fontstyle="BOLD" size="50"/>
