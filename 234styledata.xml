<?xml version='1.0' encoding='us-ascii'?>

<tunnelxml>

<fontcolours fontstyle="PLAIN" fontname="Serif">

<groupsubsetattr groupsubsetname="234planes_hierarchy" groupsubsetselectable="no">
	<subsetattr name="entarea"/>
	<subsetattr name="mainplane"/>
	<subsetattr name="pitches"/>
	<subsetattr name="lowerplane"/>
	<subsetattr name="uberpitch"/>

	<set uppersubset="entarea">
		<subsetattr name="234entrance"/>
		<subsetattr name="234lefthand"/>
	</set>
	<set uppersubset="mainplane">
		<subsetattr name="234oldtrunk"/>
		<subsetattr name="234underhand"/>
		<subsetattr name="234traverse"/>
		<subsetattr name="234trunk"/>
		<subsetattr name="234measles"/>
		<subsetattr name="234sweetsight"/>
		<subsetattr name="234sourtaste"/>
	</set>
	<set uppersubset="pitches">
		<subsetattr name="234pie"/>
		<subsetattr name="234crust"/>
		<subsetattr name="234flashgun"/>
	</set>
	<set uppersubset="lowerplane">
		<subsetattr name="234weeweeland"/>
		<subsetattr name="234wowoland"/>
		<subsetattr name="234monster"/>
		<subsetattr name="234piersquared"/>
	</set>
	<set uppersubset="uberpitch">
		<subsetattr name="234monstermunch"/>
	</set>
</groupsubsetattr>

<groupsubsetattr groupsubsetname="234planes">
	<importgroupsubsetattr groupsubsetname="default"/>
	<importgroupsubsetattr groupsubsetname="planecolours"/>
	<importgroupsubsetattr groupsubsetname="234planes_hierarchy"/>
	
	<subsetattr name="entarea" uppersubset="level1"/>
	<subsetattr name="mainplane" uppersubset="level3"/>
	<subsetattr name="pitches" uppersubset="level4"/>
	<subsetattr name="lowerplane" uppersubset="level5"/>
	<subsetattr name="uberpitch" uppersubset="level6"/>

	<subsetattr name="234grid" uppersubset="basegrid" areacolour="#88ffffff" labelcolour="#ff000000" strokecolour="#ff000000">
		<comment>Special edits to the title font sizes to get the 234 label a more sensible size for the cave.</comment>
		<labelfcol labelstylename="titlenum" fontname="Arial" size="40"/>
		<labelfcol labelstylename="titlename" fontname="Arial" size="60"/>
		<labelfcol labelstylename="titlearea" fontname="Arial" size="30"/>
		<labelfcol labelstylename="titlestats" fontname="Arial" size="30"/>
		<labelfcol labelstylename="titledetails" fontname="Arial" size="30"/>
		<labelfcol labelstylename="titlecreditslarge" fontname="Arial" size="30"/>
		<labelfcol labelstylename="titlecreditssmall" fontname="Arial" size="20"/>
		<labelfcol labelstylename="gridnum" fontname="Arial" size="15"/>
	</subsetattr>
</groupsubsetattr>

</fontcolours>
</tunnelxml>
